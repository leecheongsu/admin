<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.insrb.admin.mapper.CommonMapper">

	<update id="update">
			update ${table_name}
			   set ${column_name} = #{column_value}
			 where ${pk_column} = #{pk_value}
	</update>

</mapper>